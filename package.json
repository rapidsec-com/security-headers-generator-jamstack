{
  "name": "@rapidsec/sec-header-generator",
  "version": "1.0.0",
  "description": "Security Headers Generator for Static Sites (JAM Stack) by RapidSec",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "bin": {
    "rapidsec-headers": "dist/cjs/index.js"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/ --ext .js,.ts",
    "test": "jest",
    "format": "prettier --write \"./**/*.+(js|jsx|json|md|tsx|ts|gql|css)\""
  },
  "files": [
    "dist/",
    "package.json",
    "Readme.md",
    "LICENSE"
  ],
  "keywords": [
    "security",
    "protection",
    "csp",
    "xss",
    "clickjacking",
    "formjacking",
    "rapidsec"
  ],
  "author": "rapidsec",
  "license": "proprietary-free-to-use",
  "repository": "https://github.com/rapidsec-com/security-headers-generator-jamstack",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.20",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.set": "^4.3.6",
    "@types/node": "^14.14.27",
    "@types/node-fetch": "^2.5.8",
    "@types/sade": "^1.7.2",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ],
    "**/*.md": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "ajv": "^8.0.0",
    "debug": "^4.3.1",
    "node-fetch": "^2.6.1",
    "sade": "^1.7.4"
  }
}
